{
    "name": "sierratecnologia/operador",
    "description": "Operador para ajudar no desenvolvimento de tasks",
    "type": "library",
    "keywords": [
        "sitec",
        "operador"
    ],
    "license": "ANCAP",
    "homepage": "https://sierratecnologia.com/packages/operador",
    "support": {
        "email": "help@sierratecnologia.com.br",
        "issues": "https://github.com/sierratecnologia/operador/issues",
        "source": "https://github.com/sierratecnologia/operador",
        "docs": "https://github.com/sierratecnologia/operador/blob/master/README.md"
    },
    "authors": [
        {
            "name": "Operador",
            "homepage": "https://sierratecnologia.com.br",
            "email": "contributing@sierratecnologia.com.br"
        }
    ],
    "require": {
        "php": "^7.4",
        "privatejustice/tramite": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Operador\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [],
        "post-update-cmd": [],
        "post-create-project-cmd": []
    },
    "extra": {
        "laravel": {
            "providers": [
                "Operador\\OperadorProvider"
            ],
            "dont-discover": [
                "scaffold-interface"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
